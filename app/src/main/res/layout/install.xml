<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:orientation="horizontal"
    android:layout_marginLeft="20dp"
    android:id="@+id/linearLayout3">
    <ImageView
        android:id="@+id/back"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:src="@drawable/back"
        android:layout_gravity="center_vertical"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_marginLeft="150dp"
        android:text="设置"
        android:textColor="#333333"
        android:textSize="16sp"/>
</LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_below="@+id/linearLayout3"
        android:layout_alignParentStart="true"
        android:background="#e6e6e6"
        android:id="@+id/textView4" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/textView4"
        android:layout_alignParentEnd="true"
        android:id="@+id/linearLayout4">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="个人资料"
            android:layout_gravity="center_vertical"/>
        <ImageView
            android:id="@+id/person_info"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/details"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="295dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/linearLayout4"
        android:layout_alignParentEnd="true"
        android:id="@+id/linearLayout5">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="帐号与安全"
            android:layout_gravity="center_vertical"/>
        <ImageView
            android:id="@+id/safe"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/details"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="280dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/linearLayout5"
        android:layout_alignParentEnd="true"
        android:id="@+id/linearLayout6">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我赞过的"
            android:layout_gravity="center_vertical"/>
        <ImageView
            android:id="@+id/praise"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/details"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="295dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/linearLayout6"
        android:layout_alignParentEnd="true"
        android:id="@+id/linearLayout7">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="功能申请"
            android:layout_gravity="center_vertical"/>
        <ImageView
            android:id="@+id/function"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/details"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="295dp"/>
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#e6e6e6"
        android:layout_below="@+id/linearLayout7"
        android:layout_alignParentStart="true"
        android:id="@+id/textView5" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/textView5"
        android:layout_alignParentEnd="true"
        android:id="@+id/linearLayout8">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="隐私设置"
            android:layout_gravity="center_vertical"/>
        <ImageView
            android:id="@+id/privacy"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/details"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="295dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:layout_marginLeft="20dp"
        android:id="@+id/linearLayout9"
        android:layout_below="@+id/linearLayout8"
        android:layout_alignParentStart="true">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="新消息通知"
            android:layout_gravity="center_vertical"/>
        <ImageView
            android:id="@+id/news"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/details"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="280dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/linearLayout9"
        android:layout_alignParentEnd="true"
        android:id="@+id/linearLayout26">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="清除缓存"
            android:layout_gravity="center_vertical"/>
        <ImageView
            android:id="@+id/cache"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/details"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="295dp"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#e6e6e6"

        android:layout_below="@+id/linearLayout26"
        android:layout_alignParentStart="true"
        android:id="@+id/textView15" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="9dp"
        android:layout_below="@+id/linearLayout26"
        android:layout_alignParentEnd="true"
        android:id="@+id/linearLayout27">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="鼓励一下"
            android:layout_gravity="center_vertical"/>
        <ImageView
            android:id="@+id/encourage"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/details"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="295dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/linearLayout27"
        android:layout_alignParentEnd="true"
        android:id="@+id/linearLayout28">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="关于小红书"
            android:layout_gravity="center_vertical"/>
        <ImageView
            android:id="@+id/about"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/details"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="280dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/linearLayout28"
        android:layout_alignParentEnd="true"
        android:id="@+id/linearLayout29">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="意见反馈"
            android:layout_gravity="center_vertical"/>
        <ImageView
            android:id="@+id/advice"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/details"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="295dp"/>
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#e6e6e6"
        android:layout_below="@+id/linearLayout29"
        android:layout_alignParentStart="true" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/linearLayout29"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="10dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="退出账户"
            android:layout_gravity="center_vertical"/>
        <ImageView
            android:id="@+id/quit"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/details"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="295dp"/>
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="74dp"
        android:background="#e6e6e6"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
